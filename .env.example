CUSTOM_API_KEY=your-custom-api-key-here
CUSTOM_BASE_URL=http://localhost:11434/v1
CUSTOM_MODEL=llava:latest

# Ollama Configuration (local)
OLLAMA_BASE_URL=http://localhost:11434/v1
